{
  "version": "0.2",
  "language": "en",
  "enableFiletypes": ["typescript", "javascript", "json", "markdown", "yaml", "toml"],
  "files": ["**/*.{ts,js,json,md,yml,yaml,toml}"],
  "ignorePaths": ["docs/out/**", "dist/**", "node_modules/**", "pnpm-lock.yaml", "coverage/**", "coverage.lcov/**"],
  "dictionaryDefinitions": [
    {
      "name": "project",
      "path": "./.cspell/project.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["project"]
}
